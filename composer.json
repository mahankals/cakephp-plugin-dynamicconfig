{
  "name": "mahankals/cakephp-plugin-dynamicconfig",
  "version": "1.0.0",
  "description": "Update config from UI instead of changing fron backend & store it into database.",
  "keywords": [
    "cakephp",
    "plugin"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Atul Mahankal",
      "email": "atulmahankal@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "5.*"
  },
  "autoload": {
    "psr-4": {
      "DynamicConfig\\": "src/"
    }
  },
  "extra": {
      "cakephp-plugin": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
